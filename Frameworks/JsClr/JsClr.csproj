<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>SharpKit.JsClr</RootNamespace>
    <AssemblyName>SharpKit.JsClr</AssemblyName>
    <WarningLevel>4</WarningLevel>
    <OutputPath>bin\</OutputPath>
    <DocumentationFile>bin\$(AssemblyName).xml</DocumentationFile>
    <NoWarn>1591</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="SharpKit.Html">
      <HintPath>..\..\Defs\bin\SharpKit.Html.dll</HintPath>
    </Reference>
    <Reference Include="SharpKit.JavaScript">
      <HintPath>..\..\Defs\bin\SharpKit.JavaScript.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>
  <Import Condition="Exists('..\Common.targets')" Project="..\Common.targets" />
  <Import Project="$(SkcToolPath)\SharpKit.CSharp.targets" />
</Project>
